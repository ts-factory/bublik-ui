				"libs/bublik/features/session-log/src/index.ts"
			],
			"@/bublik/features/sidebar": [
				"libs/bublik/features/sidebar/src/index.ts"
			],
			"@/bublik/features/user-preferences": [
				"libs/bublik/features/user-preferences/src/index.ts"
			],
			"@/bublik/run-utils": ["libs/bublik/run-utils/src/index.ts"],
			"@/icons": ["libs/shared/icons/src/index.ts"],
			"@/router": ["libs/bublik/router/src/index.ts"],
			"@/services/bublik-api": ["libs/services/bublik-api/src/index.ts"],
			"@/shared/charts": ["libs/shared/charts/src/index.ts"],
			"@/shared/hooks": ["libs/shared/hooks/src/index.ts"],
			"@/shared/tailwind-ui": ["libs/shared/tailwind-ui/src/index.ts"],
			"@/shared/types": ["libs/shared/types/src/index.ts"],
			"@/shared/utils": ["libs/shared/utils/src/index.ts"]
