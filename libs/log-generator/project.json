{
	"name": "log-generator",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "libs/log-generator/src",
	"projectType": "library",
	"targets": {
		"build": {
			"executor": "@nx/js:tsc",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/libs/log-generator",
				"main": "libs/log-generator/src/index.ts",
				"tsConfig": "libs/log-generator/tsconfig.lib.json",
				"assets": ["libs/log-generator/*.md"]
			}
		},
		"publish": {
			"command": "node tools/scripts/publish.mjs log-generator {args.ver} {args.tag}",
			"dependsOn": ["build"]
		},
		"lint": {
			"executor": "@nx/eslint:lint",
			"outputs": ["{options.outputFile}"]
		},
		"test": {
			"executor": "@nx/vite:test",
			"outputs": ["{options.reportsDirectory}"],
			"options": {
				"reportsDirectory": "../../coverage/libs/log-generator"
			}
		}
	},
	"tags": []
}
